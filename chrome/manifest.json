{
  "content_scripts": [
    {
      "matches": ["https://github.com/*pull*"],
      "js": ["github-pr.js"]
    },
    {
      "matches": ["http://lite.cnn.com/*"],
      "css": ["litecnn.css"]
    },
    {
      "matches": ["https://twitter.com/*"],
      "css": ["twitter.css"]
    }
  ],
  "description": "User styles",
  "manifest_version": 2,
  "name": "chrome",
  "version": "1"
}
